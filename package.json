{
  "name": "my-app",
  "version": "0.1.0",
  "description": "Kevin Justal's Project",
  "private": true,
  "engines": {
    "node": ">=16.0"
  },
  "dependencies": {
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "customize-cra": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.12.2",
    "react-responsive": "^9.0.0-beta.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "rxjs": "^6.6.7",
    "styled-components": "^5.3.0",
    "swr": "^1.0.0",
    "use-deep-compare-effect": "^1.6.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "npm install -g serve && serve -s build",
    "dev": "npm run linter:fix && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "linter:fix": "eslint --fix ./src",
    "build:docs": "npx jsdoc -r ./src ./README.md",
    "check-update": "npm outdated --depth=0",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "jsdoc": "^3.6.7",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "react-app-rewired": "^2.1.8",
    "sass": "^1.35.1"
  }
}
